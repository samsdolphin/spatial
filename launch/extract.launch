<launch>
  <node pkg="spatial" type="extractor" name="extractor" output="screen">
    <param name="data_path" type="string" value="$(find spatial)/20210410_2/"/>
    <!-- <param name="time_start" type="double" value="1617788711.30"/>
    <param name="time_end" type="double" value="1617788837.42"/> -->
  </node>

  <arg name="rviz" default="false" />
  <group if="$(arg rviz)">
    <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find spatial)/rviz_cfg/rosbag.rviz" />
  </group>
</launch>